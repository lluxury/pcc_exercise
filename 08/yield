def get_primes(input_list):
    result_list = list()
    for element in input_list:
        if is_prime(element):
            result_list.append()

    return result_list

def get_primes2(input_list):
    return (element for element in input_list if is_prime(element))

def is_prime(number):
    if number >1:
        if number == 2:
            return True
        if number % 2 == 0:
            return False
        for current in range(3, int(math.sqrt(number)+1),2):
            #开方取整,math没定义
            if number % current == 0:
                return False
            return True
    return False
#感觉最后一个return是保险,避免内部出错误导结果

get_primes([5,6,8])
